<?xml version="1.0" encoding="UTF-8"?>
<extension key="sparkpost" type="module">
  <file>sparkpost</file>
  <name>SparkPost integration</name>
  <description>Allows CiviCRM to send emails and process bounces through the SparkPost service. Fork maintained by Symbiotic.</description>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org/extensions/sparkpost-symbiotic</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/sparkpost</url>
    <url desc="Support">https://lab.civicrm.org/extensions/sparkpost/-/issues</url>
  </urls>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Coop Symbiotic</author>
    <email>info@symbiotic.coop</email>
  </maintainer>
  <releaseDate>2021-07-07</releaseDate>
  <version>1.5</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.29</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Sparkpost</namespace>
  </civix>
</extension>
